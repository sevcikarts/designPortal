{
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "scripts": {
      "build": "yarn build",
      "start": "yarn serve"
    }
  }